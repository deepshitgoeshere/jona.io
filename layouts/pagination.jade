extends main.jade

block title
  title blog index - jona

block content
  .text.-large blog posts
  ul.blog-list
    each page in pagination.files
      li.item
        .text.-grey #{page.sdate}:
        | 
        a(href='/blog/#{page.short}')= page.title
  .text.-medium
    a(href='/') back to the index
